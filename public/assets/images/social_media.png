<script src="https://code.jquery.com/jquery-3.7.1.min.js" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.5/dist/jquery.validate.min.js" crossorigin="anonymous"></script>

<script>
  (function(){
    // PHP seeds (fallback to 1 if undefined)
    var a = parseInt('1',10),
        b = parseInt('1',10);

    function generateSum(){
      a = Math.floor(Math.random()*8)+1;
      b = Math.floor(Math.random()*8)+1;
      $("#numA").text(a); $("#numB").text(b);
      $("#sumAns").val("");
    }

    // numeric only
    $(document).on("input","#mobile,#sumAns",function(){ this.value=this.value.replace(/[^\d]/g,''); });

    $(function(){
      // validators
      $.validator.addMethod("validMobile",function(v,el){return this.optional(el)||/^[6-9]\d{9}$/.test(v);},"Enter a valid 10-digit number starting with 6–9.");
      $.validator.addMethod("sumMatch",function(v,el){var n=parseInt(v,10);return this.optional(el)||(n===(a+b));},"Wrong answer. Try again.");

      $("#refreshSum").on("click",generateSum);

      $("#enquiryForm").validate({
        ignore: [],
        rules:{
          func_area:{required:true,minlength:3},
          name:{required:true,minlength:2},
          email:{required:true,email:true},
          mobile:{required:true,validMobile:true},
          sumAns:{required:true,digits:true,sumMatch:true},
          honeypot:{maxlength:0}
        },
        messages:{
          func_area:"Please describe your requirement.",
          name:"Please enter your name.",
          email:"Please enter a valid email.",
          mobile:"Enter a valid 10-digit mobile (starts with 6–9).",
          sumAns:"Please solve the sum."
        },
        errorElement:"label",
        errorClass:"text-danger small d-block",
        highlight:function(el){$(el).addClass("is-invalid");},
        unhighlight:function(el){$(el).removeClass("is-invalid");},
        submitHandler:function(form){
          if($("#honeypot").val().trim()!==""){return false;}
          var $btn=$("#sendBtn"),$msg=$("#msg");
          $msg.empty();$btn.prop("disabled",true).text("Sending…");
          $.ajax({
            url:"/mail/enquery_footer.php",
            method:"POST",
            data:$(form).serialize(),
            dataType:"json",
            timeout:15000
          })
          .done(function(res){
            if(res && res.ok){
              $msg.html('<div class="alert alert-success">Thanks! We’ll contact you shortly.</div>');
              form.reset();generateSum();
              if(res.redirect){setTimeout(function(){location.href=res.redirect;},4000);}
            }else{
              $msg.html('<div class="alert alert-danger">Something went wrong. Please try again.</div>');
            }
          })
          .fail(function(){
            $msg.html('<div class="alert alert-danger">Network error. Please try again.</div>');
          })
          .always(function(){ $btn.prop("disabled",false).text("Send Now"); });
          return false;
        }
      });
    });

    // WhatsApp FAB
    const WA_NUMBER="919619735550";
    function buildWAPlainText(){return ["Hi Accord Exhibit,","Please arrange callback for exhibition stall design."].join("\n");}
    (function initWA(){
      var aTag=document.getElementById("wa-callback-float"); if(!aTag) return;
      var isMobile=/Android|iPhone|iPad|iPod|Opera Mini|IEMobile/i.test(navigator.userAgent);
      var msg=encodeURIComponent(buildWAPlainText());
      aTag.href=isMobile?("https://wa.me/"+WA_NUMBER+"?text="+msg):("https://web.whatsapp.com/send?phone="+WA_NUMBER+"&text="+msg);
    })();
  })();
</script>
